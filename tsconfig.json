{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "declaration": false,
    // "outDir": "./dist",
    "outDir": "./lib/",
    "strictNullChecks": true,
    "noImplicitThis": true,

    "jsx": "preserve",
    "lib": [
      "dom",
      "es2017"
    ],
    "module": "esnext",
    "moduleResolution": "node",
    "noEmit": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "preserveConstEnums": true,
    "removeComments": false,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "target": "es6",
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "types": ["cypress", "node"],
    "resolveJsonModule": true,
    "isolatedModules": true,
  },
  "baseUrl": ".", // removed src from baseurl 
  "paths": { // ensure paths were updated relative to base url 
    "@static/*": ["./public/static/*"], 
    "@generated/*": ["./src/__generated__/*"], src/store/configureStore.ts
    "@*": ["./src*"] 
    }, 
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
, "src/pages/h.js", "src/components/MessagePage/dummyMessage.js", "src/store/configureStore.js"  ],
  "exclude": [
    "node_modules"
  ],
  "typeRoots": [
    "../node_modules/@types",
    "../@types"
  ]
}
